<Project Sdk="Microsoft.NET.Sdk">

	<Import Project="$(MSBuildThisFileDirectory)../Framework.Build.props" Condition="'$(RedpointIsFrameworkImported)' != 'true'" />
	<PropertyGroup>
		<NoWarn>$(NoWarn);1591</NoWarn>
		<Nullable>enable</Nullable>
		<RootNamespace>GitHub.JPMikkers.Dhcp</RootNamespace>
	</PropertyGroup>

	<Import Project="$(MSBuildThisFileDirectory)../LibraryPackaging.Build.props" />
	<PropertyGroup>
		<Description>A modified version of the library used for the DHCP server implementation at https://github.com/jpmikkers/DHCPServer.</Description>
		<PackageTags>dhcp server</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Authors>Jean-Paul Mikkers, June Rhodes</Authors>
		<Company></Company>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" />
	</ItemGroup>

</Project>
