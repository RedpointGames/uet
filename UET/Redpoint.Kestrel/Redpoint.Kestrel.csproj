<Project Sdk="Microsoft.NET.Sdk">

	<Import Project="$(MSBuildThisFileDirectory)../Lib/Common.Build.props" />

	<Import Project="$(MSBuildThisFileDirectory)../Lib/LibraryPackaging.Build.props" />
	<PropertyGroup>
		<Description>This library provides IKestrelFactory which allows you to easily create and run Kestrel servers inside non-ASP.NET Core applications.</Description>
		<PackageTags>kestrel, https, http, self hosting</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Server.Kestrel" />
		<PackageReference Include="Microsoft.AspNetCore.WebSockets" />
	</ItemGroup>

</Project>
